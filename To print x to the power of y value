#include <stdio.h>
#include <math.h>
int main() 
{
    double x, y, result;
    printf("Enter the value of x: ");
    scanf("%lf", &x);
    printf("Enter the value of y: ");
    scanf("%lf", &y);
    result = pow(x, y);
    printf("%.2lf to the power of %.2lf is %.2lf\n", x, y, result);
 return 0;
}
